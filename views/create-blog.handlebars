<h1>Create Your Blog!</h1>

<form>
  <div class="form-group">
    <label for="blogTitle">Title</label>
    <input type="text" class="form-control" id="blogTitle" placeholder="title">
  </div>
  <div class="form-group">
    <label for="articleText">Article textarea</label>
    <textarea class="form-control" id="articleText" rows="5"></textarea>
  </div>
  <button type="submit" id="button" class="btn btn-primary">Submit</button>
</form>


<script src="/js/create-blog.js"></script>